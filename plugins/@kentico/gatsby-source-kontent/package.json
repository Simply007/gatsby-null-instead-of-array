{
  "name": "@kentico/gatsby-source-kontent",
  "version": "4.0.0-beta4",
  "description": "Gatsby source plugin for Kentico Kontent",
  "main": "./gatsby-node.js",
  "scripts": {
    "test": "jest",
    "coverage": "jest --config jest.coverage.config.js ",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "build": "babel src --out-dir ./ --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production && npm run build",
    "watch": "babel -w src --out-dir ./ --ignore **/__tests__"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kentico/gatsby-source-kontent"
  },
  "keywords": [
    "kentico-kontent",
    "gatsby-source-plugin",
    "Kentico",
    "Kentico Kontent",
    "GatsbyJS",
    "gatsby",
    "gatsby-plugin"
  ],
  "author": "Ond≈ôej Chrastina <ondrej.chrastina@gmail.com> (https://github.com/Simply007)",
  "contributors": [
    "Jan Lenoch <janl@kentico.com>"
  ],
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "@kentico/kontent-delivery": "^8.0.0",
    "change-case": "^3.1.0",
    "flatted": "^2.0.0",
    "json-stringify-safe": "^5.0.1",
    "lodash": "^4.17.15",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "rxjs": "^6.5.3"
  },
  "peerDependencies": {
    "gatsby": ">=2.18.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@kentico/kontent-test-http-service-js": "^2.1.0-beta2",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-preset-gatsby-package": "^0.1.3",
    "cross-env": "^5.2.0",
    "eslint": "^5.14.1",
    "eslint-config-google": "^0.12.0",
    "eslint-plugin-node": "^8.0.1",
    "gatsby": "^2.1.18",
    "jest": "^24.1.0",
    "regenerator-runtime": "^0.13.1"
  }
}
